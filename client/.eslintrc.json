{
  "rules": {
    "node/no-missing-import": ["error", {
        "allowModules": [],
        "resolvePaths": ["/path/to/a/modules/directory"],
        "tryExtensions": [".js", ".json", ".tsx", ".ts"]
    }],
    "rest-spread-spacing": "off"
   },
  "globals": {
    "React": true,
    "google": true,
    "mount": true,
    "mountWithRouter": true,
    "shallow": true,
    "shallowWithRouter": true,
    "context": true,
    "expect": true,
    "jsdom": true,
    "JSX": true
  }
}
